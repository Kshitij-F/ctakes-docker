FROM ubuntu:latest

RUN apt-get -y update && apt-get install openjdk-8-jdk
RUN wget -O - http://apache.osuosl.org//ctakes/ctakes-3.2.2/apache-ctakes-3.2.2-bin.tar.gz | tar xzf
RUN wget -O - http://sourceforge.net/projects/ctakesresources/files/ctakessnorx-3.2.1.1.zip | unzip
RUN wget -O - http://archive.apache.org/dist/uima//uima-as-2.4.0/uima-as-2.4.0-bin.tar.gz | tar xzf

